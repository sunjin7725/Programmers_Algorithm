    "def solution(n):\n",
    "    answer = 0\n",
    "    _str = sorted(list(str(n)))\n",
    "    \n",
    "    for idx, i in enumerate(_str):\n",
    "        answer += int(i)*(10**idx)\n",
    "    \n",
    "    return answer"
   ]
  }
