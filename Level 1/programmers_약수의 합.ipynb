    "def deviding_num(n):\n",
    "    arr = []\n",
    "    for i in range(1,n+1):\n",
    "        if n % i == 0:\n",
    "            # print(i)\n",
    "            arr.append(i)\n",
    "    return arr\n",
    "\n",
    "def solution(n):\n",
    "    return sum(deviding_num(n))"
   ]
