    "num = len(prices)\n",
    "answer = []\n",
    "count = 0\n",
    "for i in range(num):\n",
    "    count = 0\n",
    "    for j in range(i+1,num):\n",
    "        count += 1\n",
    "        if not prices[j] >= prices[i]:\n",
    "            break\n",
    "    answer.append(count)"
